    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="449*"/>
            <ColumnDefinition Width="229*"/>
        </Grid.ColumnDefinitions>
        <Button Name="create" Content="Create" HorizontalAlignment="Left" Height="60" Margin="68,291,0,0" VerticalAlignment="Top" Width="225" FontSize="20" Click="create_Click"/>
        <Label Content="Name" HorizontalAlignment="Left" Height="37" Margin="30,32,0,0" VerticalAlignment="Top" Width="146" FontSize="20"/>
        <Label Content="Last name" HorizontalAlignment="Left" Height="37" Margin="30,79,0,0" VerticalAlignment="Top" Width="146" FontSize="20"/>
        <Label Content="Age" HorizontalAlignment="Left" Height="37" Margin="30,123,0,0" VerticalAlignment="Top" Width="146" FontSize="20"/>
        <Label Content="Phone" HorizontalAlignment="Left" Height="37" Margin="30,169,0,0" VerticalAlignment="Top" Width="146" FontSize="20"/>
        <Label Content="Language" HorizontalAlignment="Left" Height="37" Margin="30,214,0,0" VerticalAlignment="Top" Width="146" FontSize="20"/>
        <TextBox Name="name" HorizontalAlignment="Left" Height="37" Margin="249,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="367" FontSize="20" VerticalContentAlignment="Center" Grid.ColumnSpan="2"/>
        <TextBox x:Name="lastName" HorizontalAlignment="Left" Height="37" Margin="249,79,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="367" FontSize="20" VerticalContentAlignment="Center" Grid.ColumnSpan="2"/>
